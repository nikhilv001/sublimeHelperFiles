vpll primeFactors(ll x){
    map<ll,ll> m;
    while(x>1){
        m[lp[x]]++;
        x/=lp[x];
    }
    vpll arr;
    for(auto x: m){
        arr.pb(x);
    }
    return arr;
}
]]></content>
